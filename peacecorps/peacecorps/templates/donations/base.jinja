<html lang='en'>
<head>
    <title>{% block title %}Peace Corps{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0,
        maximum-scale=1.0, user-scalable=0">
    <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" href="{{ static("peacecorps/css/donation.css") }}" />
    <link rel="stylesheet" type="text/css"
        href="//cloud.typography.com/6638272/662206/css/fonts.css" />
</head>
<body>
<div class="wrapper">
  <header>
    <h1>{% block header %}Peace Corps{% endblock %}</h1>
  </header>

  {% block navigation %}{% endblock %}

  <main class="{% block content_class %}{% endblock %}">
  {% block content %}{% endblock %}
  </main>

</div>
<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
{% block custom_js %}{% endblock %}

</body>
</html>

